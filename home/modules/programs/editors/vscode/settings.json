{
    "editor.fontFamily": "'JetBrainsMono Nerd Font', 'JetBrains Mono', 'monospace', monospace",
    "editor.autoClosingDelete": "never",
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "files.autoGuessEncoding": true,
    "files.insertFinalNewline": true,
    "files.simpleDialog.enable": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "catppuccin-mocha",
    "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
    "workbench.preferredLightColorTheme": "Catppuccin Latte",
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 5,
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true,
    "workbench.settings.useSplitJSON": true,
    "window.customMenuBarAltFocus": false,
    "window.dialogStyle": "custom",
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "window.openFilesInNewWindow": "default",
    "explorer.copyRelativePathSeparator": "/",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.incrementalNaming": "smart",
    "search.defaultViewMode": "tree",
    "search.showLineNumbers": true,
    "search.useGlobalIgnoreFiles": true,
    "scm.defaultViewMode": "tree",
    "extensions.autoUpdate": false,
    "terminal.external.linuxExec": "blackbox",
    "terminal.integrated.confirmOnKill": "never",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.gpuAcceleration": "canvas",
    "terminal.integrated.ignoreProcessNames": [
        "starship",
        "oh-my-posh",
        "bash",
        "zsh",
        "fish"
    ],
    "terminal.integrated.localEchoExcludePrograms": [
        "vim",
        "vi",
        "nano",
        "tmux",
        "emacs"
    ],
    "update.mode": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.emptyWindow": false,
    "catppuccin.accentColor": "lavender",
    "markdown.preferredMdPathExtensionStyle": "includeExtension",
    "markdown.preview.fontFamily": "'Noto Sans', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
    "keyboard.dispatch": "keyCode",
    "git.openRepositoryInParentFolders": "always",
    "terminal.integrated.tabFocusMode": false,
    "workbench.sideBar.location": "right",
    "findJump.dimWhenActive": true,
    "findJump.jumpChars": "hutenosadigpc.r,l'fymkwjvqz;bxHUTENOSADIGPC>R<L\"FYMKWJVQZ:BX",
    "metaGo.decoration.fontFamily": "'JetBrainsMono Nerd Font','JetBrains Mono','Operator Mono SSm Lig', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
    "metaGo.decoration.characters": "h, u, t, e, n, s, a, m, k, w, j, v, q, z",
    "metaGo.decoration.backgroundColor": "#b4befe,#bac2de",
    "metaGo.decoration.color": "#1e1e2e,#1e1e2e",
    "metaGo.decoration.borderColor": "#1e1e2e",
    "metaGo.decoration.backgroundOpacity": "1",
    "metaGo.decoration.hide.triggerKeyDownRepeatInitialDelay": 0,
    "metaGo.decoration.hide.triggerKeyDownRepeatInterval": 0,
    "metaGo.jumper.timeout": 5,
    "metaGo.jumper.commandsToUseSingleTargetChar": null,
    "metaGo.decoration.targetFollowCharBackground": "#bac2de",
    "metaGo.decoration.additionalSingleCharCodeCharacters": "H,U,T,E,N,S,A,M,K,W,J,V,Q,Z",
    "whichkey.useFullWidthCharacters": true,
    "editor.fontLigatures": true,
    "workbench.colorTheme": "Catppuccin Mocha",
    "editor.multiCursorModifier": "ctrlCmd",
    "search.exclude": {
        "**/_build": true,
        "**/.elixir_ls": true,
        "**/assets/vendor": true,
        "**/deps": true
    },
    "files.exclude": {
        "**/_build": true,
        "**/.direnv": true,
        "**/.elixir_ls": true,
        "**/.env": false,
        "**/.nix-hex": true,
        "**/.nix-mix": true,
        "**/assets/vendor": true,
        "**/deps": true
    },
    "editor.mouseWheelScrollSensitivity": 3,
    "workbench.list.mouseWheelScrollSensitivity": 3,
    "editor.minimap.enabled": false,
    "git.alwaysSignOff": true,
    "editor.rulers": [
        100
    ],
    "git.allowForcePush": true,
    "diffEditor.diffAlgorithm": "experimental",
    "git.confirmSync": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.limit.enabled": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "gitlens.rebaseEditor.ordering": "asc",
    "window.restoreWindows": "none",
    "cSpell.userWords": [
        "acked",
        "AMQP",
        "astartectl",
        "autodiscovery",
        "blobasvarchar",
        "CFSSL",
        "datastream",
        "datastreams",
        "dedup",
        "dontcare",
        "dotenv",
        "Edgehog",
        "Itertools",
        "keypair",
        "Keyspace",
        "noreply",
        "SECO",
        "ttyd",
        "unregistering",
        "upserting",
        "whereis",
        "Xandra"
    ],
    "githubPullRequests.pullBranch": "never"
}
